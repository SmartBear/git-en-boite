{
  "name": "git-en-boite-inventory",
  "version": "0.0.0",
  "description": "Adapter that holds the index of info about all repos",
  "main": "dist/index.js",
  "scripts": {
    "build:clean": "rm -rf dist *.tsbuildinfo",
    "lint:fix": "eslint '*/**/*.ts' --quiet --fix",
    "lint": "eslint '*/**/*.ts'",
    "test:wip": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip",
    "test": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip -i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartbear/git-en-boite.git"
  },
  "keywords": [
    "git"
  ],
  "author": "Matt Wynne <matt@mattwynne.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartbear/git-en-boite/issues"
  },
  "homepage": "https://github.com/smartbear/git-en-boite#readme",
  "dependencies": {
    "git-en-boite-core": "0.0.0",
    "tiny-types": "^1.16.1"
  },
  "devDependencies": {
    "hamjest": "3.7.3",
    "hamjest-sinon": "1.2.0",
    "ts-sinon": "2.0.2"
  }
}
