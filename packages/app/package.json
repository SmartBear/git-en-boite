{
  "name": "git-en-boite-app",
  "version": "0.0.0",
  "description": "Main entry point for Git-en-Bo√Æte",
  "scripts": {
    "test": "yarn test:acceptance",
    "test:wip": "yarn test:acceptance:wip",
    "test:acceptance": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 cucumber-js ../../doc/features",
    "test:acceptance:wip": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 cucumber-js ../../doc/features -p wip",
    "start:server": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:worker": "ts-node -r tsconfig-paths/register src/worker.ts",
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:worker\" -n server,worker -c green,yellow",
    "lint": "eslint '*/**/*.ts' --quiet --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/supertest": "^2.0.10",
    "bullmq": "^1.9.0",
    "concurrently": "^5.3.0",
    "git-en-boite-web": "0.0.0",
    "git-en-boite-core": "0.0.0",
    "git-en-boite-local-git": "0.0.0",
    "git-en-boite-repo-index": "0.0.0",
    "pg": "^8.3.3",
    "tiny-types": "^1.14.1",
    "ts-sinon": "^2.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/cucumber": "^6.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.4",
    "hamjest": "mattwynne/hamjest#master",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "supertest": "^5.0.0-0",
    "tmp": "^0.2.1",
    "ts-node": "9",
    "ts-node-dev": "^1.0.0-pre.62",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
