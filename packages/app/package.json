{
  "name": "git-en-boite-app",
  "version": "0.0.0",
  "description": "Main entry point for Git-en-Bo√Æte",
  "scripts": {
    "build:clean": "rm -rf dist *.tsbuildinfo",
    "test": "yarn test:acceptance",
    "test:wip": "yarn test:acceptance:wip",
    "test:acceptance": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 cucumber-js ../../doc/features",
    "test:acceptance:wip": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 cucumber-js ../../doc/features -p wip",
    "start:server": "ts-node -r tsconfig-paths/register dist/server.js",
    "start:worker": "ts-node -r tsconfig-paths/register dist/worker.js",
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:worker\" -n server,worker -c green,yellow",
    "lint": "eslint '*/**/*.ts' --quiet --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/eventsource": "^1.1.5",
    "@types/supertest": "^2.0.10",
    "bullmq": "^1.10.0",
    "concurrently": "^5.3.0",
    "eventsource": "^1.0.7",
    "get-port": "^5.1.1",
    "git-en-boite-core": "0.0.0",
    "git-en-boite-local-clones": "0.0.0",
    "git-en-boite-repo-index": "0.0.0",
    "git-en-boite-web": "0.0.0",
    "pg": "^8.4.2",
    "tiny-types": "^1.15.0",
    "typescript": "^4.0.5",
    "tsconfig-paths": "^3.9.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "ts-sinon": "^2.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.1",
    "cucumber-pretty": "^6.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "hamjest": "mattwynne/hamjest#master",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "tmp": "^0.2.1",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
