  "types": "dist/index.d.ts",
  "scripts": {
    "build:clean": "rm -rf dist *.tsbuildinfo",
    "lint:fix": "eslint '*/**/*.ts' --quiet --fix",
    "lint": "eslint '*/**/*.ts'",
    "start:server": "ts-node -r tsconfig-paths/register dist/server.js",
    "start:worker": "ts-node -r tsconfig-paths/register dist/worker.js",
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:worker\" -n server,worker -c green,yellow",
    "test:wip": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip",
    "test": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip -i"
