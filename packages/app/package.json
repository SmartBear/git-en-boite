{
  "name": "git-en-boite-app",
  "version": "0.0.0",
  "description": "Main entry point for Git-en-Bo√Æte",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:clean": "rm -rf dist *.tsbuildinfo",
    "lint:fix": "eslint '*/**/*.ts' --quiet --fix",
    "lint": "eslint '*/**/*.ts'",
    "start:server": "ts-node -r tsconfig-paths/register dist/server.js",
    "start:worker": "ts-node -r tsconfig-paths/register dist/worker.js",
    "start": "concurrently --kill-others-on-fail \"yarn start:server\" \"yarn start:worker\" -n server,worker -c green,yellow",
    "test:wip": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip",
    "test": "NODE_ENV=test TS_NODE_TRANSPILE_ONLY=1 mocha -g @wip -i"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/eventsource": "^1.1.5",
    "@types/intercept-stdout": "^0.1.0",
    "@types/rfdc": "^1.2.0",
    "bullmq": "^1.28.0",
    "concurrently": "^7.0.0",
    "error-stack-parser": "^2.0.6",
    "eventsource": "^2.0.0",
    "get-port": "^5.1.1",
    "git-en-boite-core": "0.0.0",
    "git-en-boite-inventory": "0.0.0",
    "git-en-boite-local-clones": "0.0.0",
    "git-en-boite-logging": "0.0.0",
    "git-en-boite-web": "0.0.0",
    "intercept-stdout": "^0.1.2",
    "rfdc": "^1.3.0",
    "strip-ansi": "^7.0.0",
    "tiny-types": "^1.16.1",
    "ts-eventually": "^1.0.5",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "ts-sinon": "^2.0.1",
    "tsconfig-paths": "^3.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "8.2.2",
    "@types/mocha": "9.1.0",
    "@types/node": "15.6.1",
    "@types/strip-ansi": "5.2.1",
    "@types/supertest": "2.0.12",
    "@types/triple-beam": "1.3.2",
    "cucumber-pretty": "6.0.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "hamjest": "3.7.3",
    "mocha": "9.2.2",
    "prettier": "2.6.2",
    "supertest": "6.2.2",
    "tmp": "0.2.1",
